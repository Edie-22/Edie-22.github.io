{
  "version": 2,
  "builds": [
    {
      "src": "entrypoint.sh",
      "use": "@vercel/build-utils@canary",
      "config": {
        "distDir": "public",
        "installCommand": ""
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "static/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "app.py"
    },
    {
      "src": "/(.*)",
      "dest": "app.py",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    }
  ]
}
